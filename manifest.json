{
	"manifest_version": 2,

	"name": "Spotcommander",
	"description": "Chrome App for Spotcommander",
	"version": "0.0.0",
	"icons": {
		"128": "icons/icon_128.png"
	},
	"permissions": [
		"webview",
		"notifications",
		"https://www.arago.utwente.nl/spotcommander/",
		"https://www.arago.utwente.nl/spotcommander/*",
		"http://o.scdn.co/*"

	],
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	}
}
